<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="270.0" prefWidth="412.0" xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stick_hero.ScreenController">
   <children>
      <Button fx:id="continueid" layoutX="137.0" layoutY="154.0" mnemonicParsing="false" onAction="#run" prefHeight="25.0" prefWidth="137.0" text="CONTINUE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <Button fx:id="exitid" layoutX="137.0" layoutY="205.0" mnemonicParsing="false" onAction="#switchtohomescreen" text="EXIT TO HOMESCREEN">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
